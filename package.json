  "scripts": {
    "dev": "next dev --turbopack",
    "dev:local": "pnpm run env:local && next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:start": "docker-compose -f docker-compose.simple.yml up -d",
